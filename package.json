{
  "name": "wrench-it-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build:prod": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "vercel:build": "npm run build:prod",
    "vercel:install": "npm ci --prefer-offline --no-audit",
    "postbuild": "next-sitemap",
    "clean": "rm -rf .next out node_modules",
    "clean:cache": "rm -rf .next",
    "test": "echo 'No tests configured yet'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install || true",
    "pre-commit": "npm run lint && npm run type-check && npm run format:check",
    "lighthouse": "npx lighthouse https://wrenchit.io --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
